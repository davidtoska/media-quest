{
  "name": "media-quest",
  "description": "A monorepo in TypeScript",
  "private": false,
  "workspaces": [
    "packages/builder",
    "packages/engine"
  ],
  "scripts": {
    "dev": "node dev.js",
    "tsc:watch": "tsc --watch --noEmit",
    "build:builder-lib": "cd packages/builder && npm run build",
    "build:engine": "cd packages/engine && npm run build",
    "cloc": "npx cloc packages/",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "esbuild": "^0.19.2",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@media-quest/builder": "0.0.4",
    "@media-quest/engine": "0.0.4"
  }
}
