{
  "name": "@media-quest/builder",
  "version": "0.0.27",
  "description": "Builder library for Media-quest schemas",
  "main": "dist/public-api.js",
  "types": "dist/public-api.d.js",
  "license": "MIT",
  "scripts": {
    "tsc:check:builder": "tsc --watch --noEmit",
    "clean": "rimraf dist",
    "buildXXX": "npm run clean && tsup src/public-api.ts --sourcemap inline --format cjs --dts",
    "build2": "tsup"
  },
  "tsup": {
    "entry": [
      "src/public-api.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "format": [
      "cjs"
    ],
    "dts": true
  },
  "peerDependencies": {
    "@media-quest/engine": "0.0.27"
  }
}
